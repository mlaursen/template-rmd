{
  "name": "template-rmd",
  "version": "1.0.0",
  "description": "A react-md typescript template with my preferred defaults",
  "private": true,
  "repository": "git@github.com:mlaursen/template-rmd.git",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,md,yml,json}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-static": "next build && next export"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-md": "^4.0.3"
  },
  "devDependencies": {
    "@mlaursen/eslint-config": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.38",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss,js,jsx,md,yml,json}": [
      "prettier --write"
    ]
  }
}
